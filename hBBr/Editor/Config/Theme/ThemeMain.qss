{
    height: 25px;
    font-size: 14px ;
    color:rgb(230,230,230);
    border: 1px solid rgb(180,150,150);
}
QTabBar::tab:!selected:hover 
{
    background-color: rgba(50,50,50,1);
    color:rgb(230,230,230);
}
QTabBar::tab::label 
{
    width: 120px;
}
