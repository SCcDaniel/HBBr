{
    width: 10px; 
    height: 10px; 
}
#PropertyCheckBox::indicator:checked
{   
    background:rgb(105, 255, 50);
    border:4px double rgb(20,20,20);
    border-radius:2px;		
}
#PropertyCheckBox::indicator:unchecked  
{     
    background:rgb(200,50,20);
    border:4px double rgb(20,20,20);
    border-radius:	2px;		
}
