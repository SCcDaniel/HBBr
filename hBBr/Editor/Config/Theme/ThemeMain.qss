}
/* ScrollBar */
QScrollBar:vertical
{ 
    width: 16px;
    background-color: rgba(0,0,0,0);
}
QScrollBar::handle:vertical
{
    background-color: rgba(40,40,40,1.0);
    border: 1.2px solid rgb(160, 108, 125);
    margin-left: 1px;
    margin-right: 1px;
    margin-top: 22px;
    margin-bottom: 22px;
}
QScrollBar::handle:vertical:hover
{
    background-color: rgb(160, 108, 125);
    border: 1.2px solid rgb(160, 160, 160);
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical
{
    background: rgba(0,0,0,0.5);
    margin-top: -22px;
    margin-bottom: -22px;
    border: 1px solid rgb(85, 70, 75);
}
QScrollBar::add-line:vertical
{    
    height: 20px;
    background-color: rgba(70,70,70,1);
    border: 1.2px solid rgb(180, 150, 68);
    subcontrol-position: bottom;
    margin-bottom: -15px;
}
QScrollBar::sub-line:vertical
{    
    height: 20px;
    background-color: rgba(70,70,70,1);
    border: 1.2px solid rgb(180, 150, 68);
    subcontrol-position: top;
    margin-top: -15px;
}
/*Horizontal ScrollBar*/
QScrollBar:horizontal
{ 
    height: 16px;
    background-color: rgba(0,0,0,0);
}
QScrollBar::handle:horizontal
{
    background-color: rgba(40,40,40,1.0);
    border: 1.2px solid rgb(160, 108, 125);
    margin-left: 22px;
    margin-right: 22px;
    margin-top: 1px;
    margin-bottom: 1px;
}
QScrollBar::handle:horizontal:hover
{
    background-color: rgb(160, 108, 125);
    border: 1.2px solid rgb(160, 160, 160);
}
QScrollBar::add-page:horizontal, 
QScrollBar::sub-page:horizontal
{
    background: rgba(0,0,0,0.5);
    margin-left: -22px;
    margin-right: -22px;
    border: 1px solid rgb(85, 70, 75);
}
QScrollBar::add-line:horizontal
{    
    width: 20px;
    background-color: rgba(70,70,70,1);
    border: 1.2px solid rgb(180, 150, 68);
    subcontrol-position: right;
    margin-right: -15px;
}
QScrollBar::sub-line:horizontal
{    
    width: 20px;
    background-color: rgba(70,70,70,1);
    border: 1.2px solid rgb(180, 150, 68);
    subcontrol-position: left;
    margin-left: -15px;
}

/* PushButton */
QPushButton
{
    background-color: rgba(70,70,70,1);
    color:antiquewhite;
    font-family:  Microsoft Yahei, Arial, Helvetica, sans-serif;
    border-radius: 4px;
    width: 100%;
    height: 25px;
}
QPushButton:hover
{
    background-color: rgb(130, 100, 100);
    color:antiquewhite;
    font-family:  Microsoft Yahei, Arial, Helvetica, sans-serif;
    border-radius: 4px;
    width:100%;
    height: 25px;
}
QPushButton:pressed
{
    background-color: rgba(170,120,120,1);
    color:#FFFFFF;
    font-family:  Microsoft Yahei, Arial, Helvetica, sans-serif;
    border: 1.5px solid rgb(210, 210, 210);
    border-radius: 4px;
    width:100%;
    height: 25px;
}
